    "node-sp-auth": "^2.5.2",
    "pre-commit": "^1.2.2",
    "react-hot-loader": "^4.1.2",
    "redux-devtools-extension": "^2.13.2",
    "sass-loader": "^6.0.7",
    "source-map-loader": "^0.2.3",
    "string-replace-loader": "^2.1.1",
    "style-loader": "^0.20.3",
    "ts-loader": "^5.3.3",
    "tslint": "^5.9.1",
    "tslint-loader": "^3.6.0",
    "typescript": "^3.3.3",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "\\.test\\.tsx$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ]
  },
  "pre-commit": {
    "colors": true,
    "silent": false,
    "run": [
      "test"
    ]
  },
  "author": "Kats Team",
